{
  "plugins": [
    ["annotate-pure-calls"],
    [
      "transform-rename-import",
      {
        "replacements": [
          { "original": "^fp-ts/(\\w*)$", "replacement": "fp-ts/es6/$1.js" },
          { "original": "^io-ts/(\\w*)$", "replacement": "io-ts/es6/$1.js" }
        ]
      }
    ]
  ]
}
